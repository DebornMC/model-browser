{
	"schemaVersion": 1,
	"id": "model-browser",
	"version": "${version}",
	"name": "Model Browser",
	"description": "Browse all of your favorite models loaded from your resource packs, directly in game!",
	"authors": [
		"deborn"
	],
	"license": "CC0-1.0",
	"icon": "assets/model-browser/icon.png",
	"environment": "*",
	"entrypoints": {
		"client": [
			"deborn.modelbrowser.ModelBrowserClient"
		]
	},
	"mixins": [
		"deborn.modelbrowser.mixins.json",
		{
			"config": "deborn.modelbrowser.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.3",
		"minecraft": "~1.21.10",
		"java": ">=21",
		"fabric-api": "*"
	}
}